pp.js","original":{"line":6,"column":39}}],"sources":{"components/App.js":"import React from \"react\"\n\n// API Endpoint && key\nconst asteroid_key = \"7UpN204mxX9ABjxG3j8quPfoLak8N2WGyZBaY741\"\nconst asetroid_endpoint = `https://api.nasa.gov/neo/rest/v1/neo/browse?api_key=${asteroid_key}`\nexport default class App extends React.Component {\n  constructor(props) {\n    super(props)\n    this.state = {\n      asteroids: [],\n      isLoading: false,\n      error: null\n    }\n  }\n\n  // fetch data from NASA's NEoW API\n  componentDidMount() {\n    // get loading key\n    this.setState({\n      isLoading: true\n    })\n\n    fetch(asetroid_endpoint)\n      .then((response) => {\n        if(response.ok) {\n          return(response.json())\n        } \n        throw new Error ('Ooops , something went wrong!...');\n      })\n      .then((data) =>\n        this.setState({\n          asteroids: data.asteroids,\n        })\n      ).catch(error => {\n        this.setState({\n          error, isLoading: false\n        })\n      })\n  }\n\n  render() {\n    const { asteroids, isLoading, error } = this.state\n\n    // perform react conditional rendering\n    // by displaying either a loader or text indicating that data is being fetched\n    if(isLoading) {\n      return <p>Loading...</p>\n    } \n\n    // error message\n    if(error) {\n      return <p>{error.message}</p>\n    }\n    \n    return (\n      <div className='app__base asteroids'>\n        <h3>Reveal your approaches Oh! ye asteroids ðŸ¤£ </h3>\n        {asteroids.map(asteroid => {\n          return (\n            <div key={asteroid.ObjectID}>\n              <h2>{asteroid.name}</h2>\n            </div>\n          )\n        })}\n      </div>\n    )\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"3051a8d0c64c0a07a0d28c58ebe6613a","cacheData":{"env":{}}}